{
"name": "kenanlafrance/mango-repo",
"description": "Fork of larachimp/mango-repo: Simple repository package for Laravel.",
"type": "library",
"keywords": ["php", "Laravel", "database", "repository", "pattern"],
"homepage": "https://github.com/kenanlafrance/mango-repo",
"license": "MIT",
"authors": [
    {
        "name": "Kenan Lafrance",
        "email": "your-email@example.com",
        "role": "Fork Maintainer"
    },
    {
        "name": "Percy Mamedy",
        "email": "percymamedy@gmail.com",
        "role": "Original Author"
    }
],
"require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^9.0"
},
"require-dev": {
    "mockery/mockery": "^1.4.3",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3.3"
},
"autoload": {
    "psr-4": {
        "Kenanl\\MangoRepo\\": "src/"
    }
},
"autoload-dev": {
    "psr-4": {
        "Kenanl\\MangoRepo\\Tests\\": "tests/"
    }
},
"extra": {
    "laravel": {
        "providers": [
            "Kenanl\\MangoRepo\\MangoRepoServiceProvider"
        ]
    }
},
"minimum-stability": "dev",
"prefer-stable": true
}
